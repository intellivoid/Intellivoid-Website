{
    "name": "Intellivoid Website",
    "version": "3.0.0.0",
    "author": "Zi Xing Narrakas",
    "organization": "Intellivoid Technologies",
    "configuration": {
        "root_path": "/",
        "localization": {
            "enabled": false,
            "primary_localization": "en",
            "auto_detect_preference": true,
            "localizations": {}
        },
        "sessions": {
            "enabled": true,
            "configuration_name": "Intellivoid Accounts"
        },
        "favicon": null,
        "localization_enabled": true,
        "debugging_mode": true,
        "framework_signature": true,
        "application_signature": true,
        "security_headers": true,
        "enable_khm": true,
        "firewall_deny": [],
        "headers": {}
    },
    "runtime_scripts": [
        {"event": "pre_page_load", "script": "runtime_scripts/auth.check.dyn"},
        {"event": "pre_page_load", "script": "runtime_scripts/check.host.dyn"}
    ],
    "web_assets": [
        {"type": "ppm", "name": "primary_assets", "source": "net.intellivoid.website_template==latest", "path": "assets"}
    ],
    "router": [
        {"method": ["GET"], "path": "", "page": "index", "params": []},
        {"method": ["GET"], "path": "about", "page": "about", "params": []},
        {"method": ["GET"], "path": "team", "page": "team", "params": []}
    ]
}